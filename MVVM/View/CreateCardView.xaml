             d:DesignHeight="750 " d:DesignWidth="1350">

    <UserControl.DataContext>
        <viewModel:CreateCardViewModel />
    </UserControl.DataContext>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>

        <Grid Grid.Column="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="4*"/>
                <RowDefinition Height="1*"/>
            </Grid.RowDefinitions>


            <Image x:Name="ImageDisplay" Source="/Images/Placeholder.png" />

            <StackPanel Grid.Column="0"
                        Grid.Row="1">
                <Button Content="选择图片"
                        Background="#3A6D8C"
                        Foreground="White"
                        Style="{StaticResource UploadButtonTheme}"
                        Margin="32,27,32,5" 
                        Height="50"
                        Click="UploadButton_Click"/>

                <Button Content="保存到衣柜"
                        Background="#3A6D8C"
                        Foreground="White"
                        Style="{StaticResource UploadButtonTheme}"
                        Margin="32,0,32,27" 
                        Height="50"
                        Click="SaveButton_Click"/>
            </StackPanel>
            
            
        </Grid>

        <Grid Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>

            <TextBlock Text="装备详情"
